    <artifactId>screenit-web</artifactId>
    <packaging>jar</packaging>
    <name>screenit-web</name>
    <description>Screenit webapplicatie</description>

    <build>
        <plugins>
            <plugin>
                <artifactId>maven-resources-plugin</artifactId>
                <executions>
                    <execution>
                        <id>copy-test-resources</id>
                        <phase>validate</phase>
                        <goals>
                            <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${basedir}/src/test/resources/aspose</outputDirectory>
                            <resources>
                                <resource>
                                    <directory>${basedir}/../screenit-base/src/test/resources/aspose</directory>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <artifactId>maven-antrun-plugin</artifactId>
                <executions>
                    <execution>
                        <id>set-build-time</id>
                        <phase>compile</phase>
                        <configuration>
                            <target>
                                <tstamp>
                                    <format property="timestamp" pattern="dd-MM-yyyy HH:mm:ss"/>
                                </tstamp>
                                <replace file="target/classes/nl/rivm/screenit/main/web/ScreenitApplication.properties" token="build.time=nu" value="build.time=${timestamp}"/>
                            </target>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <groupId>nl.geodienstencentrum.maven</groupId>
                <artifactId>sass-maven-plugin</artifactId>
                <configuration>
                    <resources>
                        <resource>
                            <source>
                                <directory>${basedir}/src/main/resources/static/assets/sass</directory>
                                <includes>
                                    <include>**</include>
                                </includes>
                                <excludes>
                                    <exclude>**/_svn/**</exclude>
                                    <exclude>**/.svn/**</exclude>
                                </excludes>
                            </source>
                            <destination>${project.build.directory}/classes/static/assets/css</destination>
                        </resource>
                    </resources>
                    <sassOptions>
                        <always_update>true</always_update>
                    </sassOptions>
                </configuration>
                <executions>
                    <execution>
                        <id>build</id>
                        <goals>
                            <goal>update-stylesheets</goal>
                        </goals>
                        <phase>generate-sources</phase>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>jaxb2-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <id>xjc</id>
                        <goals>
                            <goal>xjc</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <xjbSources>
                        <xjbSource>${basedir}/src/main/resources/sepa/binding-sepa.xjb</xjbSource>
                    </xjbSources>

                    <sources>
                        <source>${basedir}/src/main/resources/sepa/pain.001.001.03.xsd</source>
                    </sources>
                    <packageName>nl.rivm.screenit.generated.sepa</packageName>
                    <encoding>UTF-8</encoding>
                    <extension>true</extension>
                </configuration>
                <dependencies>
                    <dependency>
                        <groupId>xerces</groupId>
                        <artifactId>xercesImpl</artifactId>
                    </dependency>
                </dependencies>
            </plugin>

            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <goals>
                            <goal>build-info</goal>
                            <goal>repackage</goal>
                        </goals>
                        <configuration>
                            <additionalProperties>
                                <number>${buildnumber}</number>
                            </additionalProperties>
                            <outputFile>target/classes/build-info.properties</outputFile>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

