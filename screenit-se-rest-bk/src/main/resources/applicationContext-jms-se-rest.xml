<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

    <bean id="verwerkMammaSeRestListener" class="nl.rivm.screenit.mamma.se.jms.listener.SeRestMessageListener"/>

    <bean id="verwerkMammaSeRestListenerContainer" class="org.springframework.jms.listener.DefaultMessageListenerContainer">
        <property name="concurrentConsumers" value="1"/>
        <property name="connectionFactory" ref="jmsFactory"/>
        <property name="destination" ref="verwerkMammaSeRestDestination"/>
        <property name="messageListener" ref="verwerkMammaSeRestListener"/>
        <property name="sessionTransacted" value="true"/>
    </bean>
</beans>
