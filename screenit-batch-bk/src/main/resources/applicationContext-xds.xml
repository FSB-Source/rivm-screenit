<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="codetables" class="java.util.ArrayList">
        <constructor-arg>
            <list>
                <value>/bppc/screenit_AD.xml</value>
            </list>
        </constructor-arg>
    </bean>

    <bean id="codeTablesFilenames" class="java.util.ArrayList">
        <constructor-arg>
            <list>
                <value>classpath:bppc/screenit_affinity_domain.xml</value>
            </list>
        </constructor-arg>
    </bean>
    <bean id="configurationLocation" class="java.lang.String"/>

</beans>
